        eType="#//StateOperations"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="Item" eSuperTypes="#//ID">
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="expiry" lowerBound="1"
